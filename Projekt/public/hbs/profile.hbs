<div class="container target">
    <div class="row">
        <div class="col-sm-10">
             <h2 class="">My profile</h2>
             <a href="../editprofile" class="btn btn-success">Edit profile</a> <a href="../changepassword" class="btn btn-warning">Change password</a>
        </div>
    </div>
  <br>
    <div class="row">
        <div class="col-sm-3">
            <ul class="list-group">
                <li class="list-group-item text-muted" contenteditable="false">User details</li>
                <li class="list-group-item text-right"><span class="pull-left"><strong class="">Full name</strong></span>{{user.name}}</li>
                <li class="list-group-item text-right"><span class="pull-left"><strong class="">Username</strong></span>{{user.username}}</li>
                <li class="list-group-item text-right"><span class="pull-left"><strong class="">Email address</strong></span>{{user.email}}</li>
                <li class="list-group-item text-right"><span class="pull-left"><strong class="">Joined</strong></span>{{user.joined}}</li>
            </ul>
        </div>
        <!--/col-3-->
        <div class="col-sm-9" style="" contenteditable="false">
            <div class="panel panel-default">
                <div class="panel-heading">My description</div>
                <div class="panel-body"> {{user.description}}</div>
            </div>
    </div>
</div>